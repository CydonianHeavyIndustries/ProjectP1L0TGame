    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^35.7.5",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.5",
    "esbuild": "^0.25.0",
    "patch-package": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "wait-on": "^7.2.0"
